<process name="BPELProcess1"
               targetNamespace="http://xmlns.oracle.com/Flux_SOA_Process/Flux_Test5/BPELProcess1"
               xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
               xmlns:client="http://xmlns.oracle.com/Flux_SOA_Process/Flux_Test5/BPELProcess1"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/file/Flux_SOA_Process/Flux_Test5/ReadFile"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/db/Flux_SOA_Process/Flux_Test5/WriteDatabase"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:ns3="http://TargetNamespace.com/ReadingFile"
         xmlns:ns4="http://xmlns.oracle.com/pcbpel/adapter/db/top/WriteDatabase"
         xmlns:ui="http://xmlns.oracle.com/soa/designer">

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <import namespace="http://xmlns.oracle.com/pcbpel/adapter/file/Flux_SOA_Process/Flux_Test5/ReadFile"
          location="../WSDLs/ReadFile.wsdl" importType="http://schemas.xmlsoap.org/wsdl/" ui:processWSDL="true"/>
  <partnerLinks>
    <partnerLink name="ReadFile" partnerLinkType="ns1:Read_plt" myRole="Read_role"/>
    <partnerLink name="WriteDatabase" partnerLinkType="ns2:WriteDatabase_plt" partnerRole="WriteDatabase_role"/>
  </partnerLinks>
  <variables>
    <variable name="Receive1_Read_InputVariable" messageType="ns1:Read_msg"/>
    <variable name="Invoke1_insert_InputVariable" messageType="ns2:FactureCollection_msg"/>
  </variables>
  <sequence name="main">
    <receive name="Receive1" createInstance="yes" partnerLink="ReadFile" portType="ns1:Read_ptt" operation="Read"
             variable="Receive1_Read_InputVariable"/>
    <assign name="Assign1">
      <copy>
        <from>$Receive1_Read_InputVariable.body/ns3:Facture/ns3:C1</from>
        <to>$Invoke1_insert_InputVariable.FactureCollection/ns4:Facture/ns4:id</to>
      </copy>
      <copy>
        <from>$Receive1_Read_InputVariable.body/ns3:Facture/ns3:C2</from>
        <to>$Invoke1_insert_InputVariable.FactureCollection/ns4:Facture/ns4:nom</to>
      </copy>
      <copy>
        <from>$Receive1_Read_InputVariable.body/ns3:Facture/ns3:C3</from>
        <to>$Invoke1_insert_InputVariable.FactureCollection/ns4:Facture/ns4:montant</to>
      </copy>
    </assign>
    <invoke name="Invoke1" partnerLink="WriteDatabase" portType="ns2:WriteDatabase_ptt"
            operation="insert" inputVariable="Invoke1_insert_InputVariable" bpelx:invokeAsDetail="no"/>
  </sequence>
</process>