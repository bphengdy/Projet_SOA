<process name="TraitementFacture"
               targetNamespace="http://xmlns.oracle.com/Flux_SOA_Process/Flux_Test5/TraitementFacture"
               xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
               xmlns:client="http://xmlns.oracle.com/Flux_SOA_Process/Flux_Test5/TraitementFacture"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/file/Flux_SOA_Process/Flux_Test5/ReadingFile"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/db/Flux_SOA_Process/Flux_Test5/WriteDatabase"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:ns3="http://TargetNamespace.com/ReadingFile"
         xmlns:ns4="http://xmlns.oracle.com/pcbpel/adapter/db/top/WriteDatabase"
         xmlns:ui="http://xmlns.oracle.com/soa/designer">

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <import namespace="http://xmlns.oracle.com/pcbpel/adapter/file/Flux_SOA_Process/Flux_Test5/ReadingFile"
          location="../WSDLs/ReadingFile.wsdl" importType="http://schemas.xmlsoap.org/wsdl/" ui:processWSDL="true"/>
  <partnerLinks>
    <partnerLink name="ReadingFile" partnerLinkType="ns1:Read_plt" myRole="Read_role"/>
    <partnerLink name="WriteDatabase" partnerLinkType="ns2:WriteDatabase_plt" partnerRole="WriteDatabase_role"/>
  </partnerLinks>
  <variables>
    <variable name="Receive1_Read_InputVariable" messageType="ns1:Read_msg"/>
    <variable name="Invoke1_insert_InputVariable" messageType="ns2:FactureCollection_msg"/>
  </variables>
  <sequence name="main">
    <receive name="Receive1" partnerLink="ReadingFile" portType="ns1:Read_ptt" operation="Read" createInstance="yes"
             variable="Receive1_Read_InputVariable"/>
    <assign name="Transformation1">
      <bpelx:annotation>
        <bpelx:pattern patternName="bpelx:transformation"></bpelx:pattern>
      </bpelx:annotation>
      <copy>
        <from>ora:doXSLTransformForDoc("../Transformations/Transformation_2.xsl", $Receive1_Read_InputVariable.body)</from>
        <to variable="Invoke1_insert_InputVariable" part="FactureCollection"/>
      </copy>
    </assign>
    <invoke name="Invoke1" bpelx:invokeAsDetail="no" partnerLink="WriteDatabase" portType="ns2:WriteDatabase_ptt"
            operation="insert" inputVariable="Invoke1_insert_InputVariable"/>
  </sequence>
</process>