<process name="TraitementFacture"
               targetNamespace="http://xmlns.oracle.com/Flux_SOA_Process/FluxFacture/TraitementFacture"
               xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
               xmlns:client="http://xmlns.oracle.com/Flux_SOA_Process/FluxFacture/TraitementFacture"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/file/Flux_SOA_Process/FluxFacture/ReadFile"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/db/Flux_SOA_Process/FluxFacture/WriteToDB"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:bpel2="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ns3="http://TargetNamespace.com/ReadingFile"
         xmlns:ns4="http://xmlns.oracle.com/pcbpel/adapter/db/top/WriteToDB">

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <bpelx:annotation>
    <bpelx:analysis>
      <bpelx:property name="wsdlFile">
        <![CDATA[../WSDLs/ReadFile.wsdl]]>
      </bpelx:property>
    </bpelx:analysis>
  </bpelx:annotation>
  <partnerLinks>
    <partnerLink name="ReadFile" partnerLinkType="ns1:Read_plt" myRole="Read_role"/>
    <partnerLink name="WriteToDB" partnerLinkType="ns2:WriteToDB_plt" partnerRole="WriteToDB_role"/>
  </partnerLinks>
  <variables>
    <variable name="Receive1_Read_InputVariable" messageType="ns1:Read_msg"/>
    <variable name="Invoke1_insert_InputVariable" messageType="ns2:FactureCollection_msg"/>
  </variables>
  <sequence name="main">
    <receive name="Receive1" createInstance="yes" variable="Receive1_Read_InputVariable" partnerLink="ReadFile"
             portType="ns1:Read_ptt" operation="Read"/>
    <assign name="Assign1">
      <copy>
        <from variable="Receive1_Read_InputVariable" part="body" query="/ns3:Factures/ns3:Facture/ns3:C1"/>
        <to variable="Invoke1_insert_InputVariable" part="FactureCollection"
            query="/ns4:FactureCollection/ns4:Facture/ns4:id"/>
      </copy>
      <copy>
        <from variable="Receive1_Read_InputVariable" part="body" query="/ns3:Factures/ns3:Facture/ns3:C2"/>
        <to variable="Invoke1_insert_InputVariable" part="FactureCollection"
            query="/ns4:FactureCollection/ns4:Facture/ns4:nom"/>
      </copy>
      <copy>
        <from variable="Receive1_Read_InputVariable" part="body" query="/ns3:Factures/ns3:Facture/ns3:C3"/>
        <to variable="Invoke1_insert_InputVariable" part="FactureCollection"
            query="/ns4:FactureCollection/ns4:Facture/ns4:montant"/>
      </copy>
    </assign>
    <invoke name="Invoke1" inputVariable="Invoke1_insert_InputVariable" partnerLink="WriteToDB"
            portType="ns2:WriteToDB_ptt" operation="insert"/>
  </sequence>
</process>