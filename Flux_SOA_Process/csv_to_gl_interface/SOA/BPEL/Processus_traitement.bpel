<process name="Processus_traitement"
               targetNamespace="http://xmlns.oracle.com/Flux_SOA_Process/csv_to_gl_interface/Processus_traitement"
               xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
               xmlns:client="http://xmlns.oracle.com/Flux_SOA_Process/csv_to_gl_interface/Processus_traitement"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/file/Flux_SOA_Process/csv_to_gl_interface/fichier_csv"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/db/Flux_SOA_Process/csv_to_gl_interface/vers_GL_INTERFACE"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension" xmlns:ui="http://xmlns.oracle.com/soa/designer">

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <import namespace="http://xmlns.oracle.com/pcbpel/adapter/file/Flux_SOA_Process/csv_to_gl_interface/fichier_csv"
          location="../WSDLs/fichier_csv.wsdl" importType="http://schemas.xmlsoap.org/wsdl/" ui:processWSDL="true"/>
  <partnerLinks>
    <partnerLink name="fichier_csv" partnerLinkType="ns1:Read_plt" myRole="Read_role"/>
    <partnerLink name="vers_GL_INTERFACE" partnerLinkType="ns2:vers_GL_INTERFACE_plt"
                 partnerRole="vers_GL_INTERFACE_role"/>
  </partnerLinks>
  <variables>
    <variable name="Receive1_Read_InputVariable" messageType="ns1:Read_msg"/>
    <variable name="Invoke1_insert_InputVariable" messageType="ns2:GlInterfaceCollection_msg"/>
  </variables>
  <sequence name="main">
    <receive name="Receive1" partnerLink="fichier_csv" portType="ns1:Read_ptt" operation="Read"
             variable="Receive1_Read_InputVariable" createInstance="yes"/>
    <assign name="Transformation1">
      <bpelx:annotation>
        <bpelx:pattern patternName="bpelx:transformation"></bpelx:pattern>
      </bpelx:annotation>
      <copy>
        <from>ora:doXSLTransformForDoc("../Transformations/Transformation_1.xsl", $Receive1_Read_InputVariable.body)</from>
        <to variable="Invoke1_insert_InputVariable" part="GlInterfaceCollection"/>
      </copy>
    </assign>
    <invoke name="Invoke1" partnerLink="vers_GL_INTERFACE"
            portType="ns2:vers_GL_INTERFACE_ptt" operation="insert" inputVariable="Invoke1_insert_InputVariable"
            bpelx:invokeAsDetail="no"/>
  </sequence>
</process>